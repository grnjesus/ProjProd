@model IEnumerable<ProjProd.Models.Discounts>
@{
    ViewBag.Title = "Все купоны";
}

<h2>Все купоны</h2>
<p>
    @Html.ActionLink("Добавить купон", "Create")
</p>
<table class="table">
    <tr>
        <th>Купон</th>
        <th>Значение</th>
        <th>Тип</th>
        <th>Количество</th>
        <th>Дата окончания</th>
        <th>Примечание</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name_discount)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Value_discount)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Type_DiscountID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Quantity)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.End_date)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Note)
    </td>
    <td>
        @Html.ActionLink("Редактировать", "Edit", new { id = item.ID }) |
        @Html.ActionLink("Удалить", "Delete", new { id = item.ID })
    </td>
</tr>
    }
</table>